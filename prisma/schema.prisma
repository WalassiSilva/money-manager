generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id         Int       @id @default(autoincrement())
  title      String?   @db.VarChar(50)
  value      Float?    @db.Real
  day        DateTime? @db.Date
  n_category Int?      @db.SmallInt
  category   String?   @db.VarChar(50)
  type       Int?      @db.SmallInt

  @@map("transactions")
}
